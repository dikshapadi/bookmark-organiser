{
    "manifest_version": 3,
    "name": "Bookmark Organizer",
    "version": "1.0",
    "permissions": [
      "bookmarks",
      "storage",
      "tabs",
      "activeTab"
      
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_title": "Bookmark Organizer"
    },
    "icons": {
      "16": "logo.png",
      "48": "logo.png",
      "128": "logo.png"
    }
  
  }
  